<%@ Page Language="C#" MasterPageFile="MasterPage.Master" AutoEventWireup="True" CodeBehind="ProductExport.aspx.cs" Inherits="SocoShop.Web.Admin.ProductExport" %><%@ Register Namespace="SkyCES.EntLib" Assembly="SkyCES.EntLib" TagPrefix="SkyCES"%><%@ Import Namespace="SocoShop.Common" %><%@ Import Namespace="SocoShop.Business" %><%@ Import Namespace="SocoShop.Entity" %><asp:Content ID="MasterContent" ContentPlaceHolderID="ContentPlaceHolder" runat="server"><div class="position"><img src="/Admin/Style/Images/PositionIcon.png"  alt=""/>商品导出</div><div class="add">	<ul>		<li class="left">数据格式：</li>		<li class="right"><asp:DropDownList ID="DataFormat" runat="server" onchange="changeDataFormat(this.value)"><asp:ListItem Value="1" Text="导出淘宝支持数据格式"></asp:ListItem><asp:ListItem Value="2" Text="导出拍拍支持数据格式"></asp:ListItem></asp:DropDownList></li>	</ul>	<ul id="ClassIDDiv">		<li class="left">宝贝栏目ID：</li>		<li class="right"><SkyCES:TextBox ID="ClassID" CssClass="input" runat="server" Width="100px" Text="0" CanBeNull="必填" RequiredFieldType="数据校验"/><br />宝贝栏目ID为淘宝分类的ID，如若不确定，请填写0</li>	</ul>		<ul>		<li class="left">平邮价格：</li>		<li class="right"><SkyCES:TextBox ID="Common" CssClass="input" runat="server" Width="100px" Text="0" CanBeNull="必填" RequiredFieldType="数据校验"/> 元 </li>	</ul>	<ul>		<li class="left">快递价格：</li>		<li class="right"><SkyCES:TextBox ID="Fast" CssClass="input" runat="server" Width="100px" Text="0" CanBeNull="必填" RequiredFieldType="数据校验"/> 元 </li>	</ul>	<ul id="EMSDiv">		<li class="left">EMS价格：</li>		<li class="right"><SkyCES:TextBox ID="EMS" CssClass="input" runat="server" Width="100px" Text="0" CanBeNull="必填" RequiredFieldType="数据校验"/> 元 </li>	</ul>		</div><div class="action">    <asp:Button CssClass="button" ID="SubmitButton" Text=" 确 定 " runat="server"  OnClick="SubmitButton_Click" /></div><script language="javascript" type="text/javascript">function changeDataFormat(dataFormat){    if(dataFormat=="1"){        o("ClassIDDiv").style.display="";        o("EMSDiv").style.display="";    }    else{        o("ClassIDDiv").style.display="none";        o("EMSDiv").style.display="none";    }}</script></asp:Content>