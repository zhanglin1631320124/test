<%@ Page Language="C#" MasterPageFile="MasterPage.Master" AutoEventWireup="True" CodeBehind="UserApplyAdd.aspx.cs" Inherits="SocoShop.Web.Admin.UserApplyAdd" %><%@ Register Namespace="SkyCES.EntLib" Assembly="SkyCES.EntLib" TagPrefix="SkyCES"%><asp:Content ID="MasterContent" ContentPlaceHolderID="ContentPlaceHolder" runat="server"><div class="position"><img src="/Admin/Style/Images/PositionIcon.png"  alt=""/>提现审核</div><div class="add">	<ul>		<li class="left">流水号：</li>		<li class="right"><%=userApply.Number%></li>	</ul>	<ul>		<li class="left">提现金额：</li>		<li class="right"><%=userApply.Money %></li>	</ul>	<ul>		<li class="left">提现信息：</li>		<li class="right"><%=userApply.UserNote %></li>	</ul>	<ul>		<li class="left">状态：</li>		<li class="right"><asp:RadioButtonList ID="Status" runat="server" RepeatDirection="Horizontal"><asp:ListItem Value="1">处理中</asp:ListItem><asp:ListItem Value="2">已完成</asp:ListItem><asp:ListItem Value="3">取消</asp:ListItem></asp:RadioButtonList></li>	</ul>	<ul>		<li class="left">备注：</li>		<li class="right"><SkyCES:TextBox ID="AdminNote" CssClass="input" runat="server" Width="300px"  TextMode="MultiLine" Height="100px"/></li>	</ul>	</div><div class="action">    <asp:Button CssClass="button" ID="SubmitButton" Text=" 确 定 " runat="server"  OnClick="SubmitButton_Click" /></div></asp:Content>